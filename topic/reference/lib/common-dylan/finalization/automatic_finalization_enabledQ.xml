<dylanFunction id="lib-common-dylan-finalization-automatic_finalization_enabledQ">
  <apiName>automatic-finalization-enabled?</apiName>

  <shortdesc>Returns true if automatic finalization is enabled, and
  false otherwise.</shortdesc>

  <prolog>
    <author></author>
    <copyright>
      <copyryear year="2005"/>
      <copyrholder>Gwydion Dylan Maintainers</copyrholder>
    </copyright>
  </prolog>

  <dylanFunctionDetail>
    <dylanFunctionDef>
      <dylanFunctionReturn>
	<apiItemName>enabled?</apiItemName>
        <apiOperationClassifier href="../../dylan/dylan/LbooleanG.xml">&lt;boolean&gt;</apiOperationClassifier>
      </dylanFunctionReturn>
    </dylanFunctionDef>

    <apiDesc>
      <p>Returns true if automatic finalization is enabled, and false
      otherwise.  The default value is <codeph>#f</codeph>.</p>
    </apiDesc>
  </dylanFunctionDetail>
</dylanFunction>

<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE dylanGenericFunction PUBLIC "-//Gwydion//DTD DITA Dylan API Generic Function//EN" "../../../../dtd/dylanGenericFunction.dtd" []>
<dylanGenericFunction id="lib-common-dylan-common-extensions-remove_all_keysX">
  <apiName>remove-all-keys!</apiName>
  <shortdesc>Removes all keys from a collection and leaves it empty.</shortdesc>

  <prolog>
    <author></author>
    <copyright>
      <copyryear year="2005"/>
      <copyrholder>Gwydion Dylan Maintainers</copyrholder>
    </copyright>
  </prolog>

  <dylanGenericFunctionDetail>
    <dylanGenericFunctionDef>
      <dylanGenericFunctionSealing value='open'/>
      <dylanFunctionParam>
	<apiItemName>c</apiItemName>
        <apiOperationClassifier href="../../dylan/dylan/Lmutable_explicit_key_collectionG.xml">&lt;mutable-explicit-key-collection&gt;</apiOperationClassifier>
	<apiDefNote></apiDefNote>
      </dylanFunctionParam>
      <dylanFunctionRestReturn>
	<apiItemName>results</apiItemName>
        <apiOperationClassifier href="../../dylan/dylan/LobjectG.xml">&lt;object&gt;</apiOperationClassifier>
	<apiDefNote></apiDefNote>
      </dylanFunctionRestReturn>
    </dylanGenericFunctionDef>

    <apiDesc>
      <p>Modifies <parmname>collection</parmname> by removing all its
      keys and elements, and leaves it empty. </p>

      <note>To empty collections that are not instances of
      <apiclassifier
      href="../../dylan/dylan/Lmutable_explicit_key_collectionG.xml">&lt;mutable-explicit-key-collection&gt;</apiclassifier>,
      use <apioperation
      href="../../dylan/dylan/size_setter.xml">size-setter</apioperation>.</note>
    </apiDesc>
  </dylanGenericFunctionDetail>
</dylanGenericFunction>

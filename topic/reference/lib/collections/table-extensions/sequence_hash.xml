<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE dylanFunction PUBLIC "-//Gwydion//DTD DITA Dylan API Function//EN" "../../../../dtd/dylanFunction.dtd" []>
<dylanFunction id="lib-collections-table-extensions-sequence_hash">
  <apiName>sequence-hash</apiName>

  <shortdesc>Returns a hash value for the elements of a
  sequence.</shortdesc>

  <prolog>
    <author></author>
    <copyright>
      <copyryear year="2005"/>
      <copyrholder>Gwydion Dylan Maintainers</copyrholder>
    </copyright>
  </prolog>

  <dylanFunctionDetail>
    <dylanFunctionDef>
      <dylanFunctionParam>
	<apiItemName>element-hash</apiItemName>
        <apiOperationClassifier href="../../dylan/dylan/LfunctionG.xml">&lt;function&gt;</apiOperationClassifier>
	<apiDefNote></apiDefNote>
      </dylanFunctionParam>
      <dylanFunctionParam>
	<apiItemName>seq</apiItemName>
        <apiOperationClassifier href="../../dylan/dylan/LsequenceG.xml">&lt;sequence&gt;</apiOperationClassifier>
	<apiDefNote></apiDefNote>
      </dylanFunctionParam>
      <dylanFunctionParam>
	<apiItemName>hash-state</apiItemName>
        <apiOperationClassifier href="Lhash_stateG.xml">&lt;hash-state&gt;</apiOperationClassifier>
	<apiDefNote></apiDefNote>
      </dylanFunctionParam>
      <dylanFunctionReturn>
	<apiItemName>id</apiItemName>
        <apiOperationClassifier href="../../dylan/dylan/LintegerG.xml">&lt;integer&gt;</apiOperationClassifier>
	<apiDefNote></apiDefNote>
      </dylanFunctionReturn>
      <dylanFunctionReturn>
	<apiItemName>state</apiItemName>
        <apiOperationClassifier href="Lhash_stateG.xml">&lt;hash-state&gt;</apiOperationClassifier>
	<apiDefNote></apiDefNote>
      </dylanFunctionReturn>
    </dylanFunctionDef>

    <apiDesc>
      <p>Hashes every element of <parmname>sequence</parmname> using
      <parmname>elt-hash-function</parmname>, and merges the resulting
      hash codes in order.</p>

      <p>The function <parmname>elt-hash-function</parmname> must be
      suitable for use as a hash function, as described in the
      <cite>Dylan Reference Manual</cite>.</p>

      <p>The <parmname>ordered</parmname> keyword is passed on to
      <apioperation
      href="../../dylan/dylan/merge_hash_ids.xml">merge-hash-ids</apioperation>.</p>
    </apiDesc>
  </dylanFunctionDetail>
</dylanFunction>
